{
  "manifest_version": 2,

  "name": "Coursera Helper",
  "description": "This extension helps to organize your time on Coursera",
  "version": "1.1",

  "browser_action": {
    "default_icon": "coursera_icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage", "tabs", "activeTab", "https://*.coursera.org/*"
  ]
}